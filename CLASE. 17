#include <stdio.h>

int main() 
{
    int longTOTAL, rollosde500, rollosde300, rollosde75, metrosFaltantes;
    
    printf("Longitud total alambre(en metros): ");
    scanf("%d", &longTOTAL);

    rollosde500 = longTOTAL / 500;
    longTOTAL %= 500; 
    
    rollosde300 = longTOTAL / 300;
    longTOTAL %= 300; 
    
    rollosde75 = longTOTAL / 75;
    longTOTAL %= 75; 
    
    metrosFaltantes = longTOTAL;
    
    printf("Número de rollos de 500 metros: %d\n", rollosde500);
    printf("Número de rollos de 300 metros: %d\n", rollosde300);
    printf("Número de rollos de 75 metros: %d\n", rollosde75);
    printf("Metros de alambre faltantes: %d\n", metrosFaltantes);
    getch();
    getch ();
    return 0 ;
}
