#include<stdio.h>

int main() 
{
	float dato1,dato2,dividir,multiplicar, resta,suma;
	printf("Ingrese dato 1\n");
	scanf("%f",&dato1);
	printf("Ingrese dato 2\n");
	scanf("%f",&dato2);
	suma = (dato1+dato2);
	resta = (dato1-dato2);
	multiplicar = (dato1*dato2);
	dividir = (dato1/dato2);
	printf("La suma es:%f\n",suma);
	printf("La resta es:%f\n",resta);
	printf("La multiplicacion es:%f\n",multiplicar);
	printf("La divicion es:%f\n",dividir);
	getch ();
	return 0;
}
