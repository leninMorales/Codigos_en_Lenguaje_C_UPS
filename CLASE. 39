#include <stdio.h>
int main() 
{
    int n, a = 0, b = 1, c, i;
    printf("Ingrese el número de términos (n): ");
    scanf("%d", &n);
    printf("Serie de Fibonacci hasta %d términos:\n", n);
    printf("%d ", a);
    for (i = 1; i < n; i++) 
	{
        printf("%d ", b);
        c = a + b;
        a = b;
        b = c;
    }
    printf("\n");
    getch ();
    return 0;
}
