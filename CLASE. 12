#include<stdio.h>
int main()
{
    int num1,num2,num3;
    printf("Ingrese el primer numero:");
    scanf("%d",&num1);
    printf("Ingrese el segundo numero:");
    scanf("%d",&num2);
    printf("Ingrese el tercer numero:");
    scanf("%d",&num3);
    if (num1>num2)
    {
        if(num1>num3)
        {
        printf("numero mayor:%d",num1); 
        }
        else 
        {
        printf("numero mayor:%d",num3);     
        }
    }   
    else     
    {
        if(num2>num3)
        {
            printf("numero mayor:%d",num2);
        }
        else
        {
            printf("numero mayor:%d",num3);
        }       
    }
    getch();
    return 0;
}
